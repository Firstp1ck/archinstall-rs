# Example limine.conf generated by archinstall-rs
# timeout in seconds
timeout: 5

/Arch Linux (linux)
    protocol: linux
    path: uuid(BOOT_UUID):/vmlinuz-linux
    cmdline: root=/dev/disk/by-uuid/ROOT_UUID rw quiet
    module_path: uuid(BOOT_UUID):/initramfs-linux.img

/Arch Linux (linux-fallback)
    protocol: linux
    path: uuid(BOOT_UUID):/vmlinuz-linux
    cmdline: root=/dev/disk/by-uuid/ROOT_UUID rw quiet
    module_path: uuid(BOOT_UUID):/initramfs-linux-fallback.img
